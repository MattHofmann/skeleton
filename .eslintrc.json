{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "indent": ["error", "tab"],
        "no-tabs": 0,
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-unused-vars": [
            "error",
            {
                "vars": "local",
                "args": "none"
            }
        ],
        "func-names": ["error", "never"]
    },
    "globals": {
        "google": true,
        "jquery": true,
        "$": true
    }
}